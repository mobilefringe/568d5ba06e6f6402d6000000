<script>
    $(function(e) {
        init(e);
        
        var data = [];
        $.ajax({
            url: "//mallmaverick.com/posts/ibeacon-test.json",
            method: "GET",
            dataType: "json",
        }).then(function(data) {
            console.log(data);
            $('#mm_post_title').html(data.title);
            $('#mm_post_author').html(data.author);
            $('#mm_post_image').html(data.image_file_name);
            $('#mm_post_content').html(data.body);
        });
    
    });
</script>