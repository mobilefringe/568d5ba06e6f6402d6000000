    <body id="top">
            <section id="header" class="headroom">
                <div class="main_container">
                    <a href="/">
                        <img src="//codecloud.cdn.speedyrails.net/sites/568d5ba06e6f6402d6000000/image/png/1494521541000/mm_logo1.2-white.png" class="main_logo" alt="logo">
                    </a>
                    <img src="//codecloud.cdn.speedyrails.net/sites/568d5ba06e6f6402d6000000/image/png/1455216554000/ic_menu_black_24px.png" class="menu_toggler show_phone" alt="toggler">
                    <ul class="custom_nav" id="menu">
                        <li><a href="#" class="" id="open_features">Features <i class="fa fa-chevron-down"></i></a></li>
                        <li><a href="/faq">FAQ</a></li>
                        <li><a href="/contact">Contact</a></li>
                        <li><a href="/posts">Blog</a></li>
                    </ul>
                    <div id="features_menu">
                        <div class="full_width" id="feature_insider">
                            <div class="row">
                                <div class="col-md-3">
                                    <h4 class="heading menu_title">Features</h4>
                                </div>
                                <div class="col-md-9">
                                    <ul id="feature_page_container" class="feature_menu">
                                        <script id="feature_page_template" type="x-tmpl-mustache/text">
                                            {% raw %}
                                                <li><a href="/posts/{{slug}}" class="contact">{{title}}</a></li>
                                            {% endraw %}
                                        </script> 
                                    </ul>    
                                </div>  
                            </div>
                        </div>
                    </div>
                </div>
                <div class="custom_mobile_menu show_phone">
                    <div class="col-xs-4">
                        <a class="active" href="#">Features <i class="fa fa-chevron-down"></i></a>
                    </div>
                    <div class="col-xs-4 text-center">
                        <a href="/faq">FAQ</a>
                    </div>
                    <div class="col-xs-4 text-right">
                        <a href="/contact">Contact</a>
                    </div>
                    <div>
                        <a class="footer_menu_options" href="/posts">Blog</a>
                    </div>
                    <div class="col-xs-12">
                        <ul id="mobile_feature_container" class="mobile_submenu">
                            <script id="mobile_feature_template" type="x-tmpl-mustache/text">
                                {% raw %}
                                    <li><a href="/posts/{{slug}}" class="contact">{{title}}</a></li>
                                {% endraw %}
                            </script> 
                        </ul>
                    </div>
                </div>
                
            </section>
        {{yield}}
        <section id="footer">
            <div id="site_map">
                <div class="footer_menu">
                    <div class="main_container hidden_phone">
                        <a id="menu_features" class="footer_menu_options" href="#">Features</a>
                        <a class="footer_menu_options" href="/faq">FAQ</a>
                        <a class="footer_menu_options" href="/contact">Contact</a>
                        <a class="footer_menu_options" href="/posts">Blog</a>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="main_container">
                    <div class="row">
                        <div class="col-sm-2 col-sm-push-6">
                            <div class="stay_connected">
                                <p>Stay Connected:</p>
                                <a href="https://www.facebook.com/MallMaverick" target="_blank">
                                    <img src="//codecloud.cdn.speedyrails.net/sites/568d5ba06e6f6402d6000000/image/png/1452876077000/facebook.png" class="" alt="facebook">
                                </a>
                                <a href="https://twitter.com/MallMaverick" target="_blank">
                                    <img src="//codecloud.cdn.speedyrails.net/sites/568d5ba06e6f6402d6000000/image/png/1452876062000/twitter.png" class="" alt="twiiter">
                                </a>
                            </div>
                        </div>
                        <div class="col-sm-4 col-sm-push-6">
                            <img src="//codecloud.cdn.speedyrails.net/sites/568d5ba06e6f6402d6000000/image/png/1452876539000/zendesk.png" class="zendesk" alt="zendesk">
                        </div>
                        <div class="mobile_footer_menu show_phone">
                            <a id="menu_features" class="footer_menu_options" href="#">Features</a>
                        </div>
                        <div class="col-sm-6 col-sm-pull-6">
                            <div class="row">
                                <div class="col-sm-12 footer_features">
                                    <ul id="footer_container" class="footer_menu_list">
                                        <script id="footer_template" type="x-tmpl-mustache/text">
                                            {% raw %}
                                                <li><a href="/posts/{{slug}}" class="contact">{{title}}</a></li>
                                            {% endraw %}
                                        </script> 
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="main_container bottom_menu_footer">
                <div class="show_phone">
                    <p>&copy;<span id="current_year" class="current_year"></span> Mall Maverick. All rights reserved.</p>
                    <p>
                        <a href="http://mobilefringe.com/pages/mobilefringe-privacy-policy" target="_blank">Privacy Policy</a>
                    </p>
                    <br/>
                    <p>
                        Powered by <a href="//mobilefringe.com" target="_blank" class="mm_link">Mobile Fringe</a>
                    </p>
                </div>
                <p class="pull-left hidden_phone">
                    &copy; <span id="current_year" class="current_year"></span> Mall Maverick. All rights reserved.
                    <a href="http://mobilefringe.com/pages/mobilefringe-privacy-policy" target="_blank">Privacy Policy</a>
                </p>
                <p class="pull-right hidden_phone">
                    Powered by <a href="//mobilefringe.com" target="_blank" class="mm_link">Mobile Fringe</a>
                </p>
            </div>
        </section>
